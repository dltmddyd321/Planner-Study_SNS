<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/initInfoButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="260dp"
        android:layout_marginEnd="63dp"
        android:text="회원정보"
        app:layout_constraintBaseline_toBaselineOf="@+id/updateUserInfo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/updateUserInfo"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/updateUserInfo"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="188dp"
        android:layout_marginEnd="71dp"
        android:text="개인정보수정"
        app:layout_constraintEnd_toStartOf="@+id/initInfoButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>